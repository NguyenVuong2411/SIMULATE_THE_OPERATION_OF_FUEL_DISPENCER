#include <thaygia.h>
#include<keypadcode.c>
UNSIGNED INT16 m1;
float b,t;
UNSIGNED INT8 CHUOI2[] = {"                "};
NE:  
   KEY_4X4();
   if(MP!=0x0FF)
   {
      if(MP<11) 
         {   m1 = m1++;
             t = t*(10^(m1)) + MP;
             
           
         for(a2=0; a2<16; a2++)
            {
               CHUOI2[a2]=CHUOI2[a2+1];
               
               }
           
               CHUOI2[15]=MP+0x30;
               
      /////////////////////
               lcd_gotoxy(21,2);
               for (a2=0; a2<16; a2++)
               {lcd_putc(CHUOI2[a2]);
               }
             
               }
               else if (MP == 24)
               { b= t;  }
               goto NE; }
               

     
